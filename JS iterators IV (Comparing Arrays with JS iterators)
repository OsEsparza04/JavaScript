let arrayOne=[1,3,4,5,6,7,8,9];
let arrayTwo=[1,3,4,5,6,7,9];

arrayOne.forEach (one=>{
  arrayTwo.forEach (two=>{
    if(one===two){
      console.log('Match :'+ one); 
    }
  });
});


let arrayOne=[1,3,4,5,6,7,8,9];
let arrayTwo=[1,3,4,5,6,7,9];
let arrayThree=[1,3,4];

arrayOne.forEach (one=>{
  arrayTwo.forEach (two=>{
    arrayThree.forEach(three =>{
    if(one===two && one===three){
      console.log('Match :'+ one); 
    }
  });
});
});
       
